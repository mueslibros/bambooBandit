            <kind>1</kind>
            <string>/// move

// reset
inX = 0;
inY = 0;
ilX = 0;
ilY = 0;
xChange = 0;


// X
if (keyboard_check(vk_left)) { inX = -1; ilX = -1 }
else if (keyboard_check(vk_right)) { inX = 1; ilX = 1 }

// Y
if (keyboard_check(vk_up))        { inY = -1; ilY = -1 }
else if (keyboard_check(vk_down)) { inY = 1; ilY = 1}



if (x = xTarget and ilX != 0){

    var checkDist;

    if (xTarget &gt;= xStart and ilX = 1) checkDist = gridSizeX
    else                               checkDist = gridSizeX

    if (!position_meeting(xTarget+ilX*checkDist, yTarget, bamboo)){
        xTarget += ilX*gridSizeX
        xTarget = round(xTarget/gridSizeX)*gridSizeX
    }

    
    ilX = 0
}



// update target




// change target y
if (x != xTarget){
    var difX = xTarget - x;
    var mvSpd = clamp(abs(difX), 1, 8)*sign(difX);
    
    x += mvSpd
    
    if (x &gt; xStart) {
        x = xStart
        xTarget -= mvSpd
        
        if (abs(difX) &lt;= 1){
            xTarget = x
            xChange = difX
        }
        else {
            xChange = mvSpd
        }
    }
    else {
        if (abs(difX) &lt;= 1) {
            x = xTarget
        }
    }
}






// change target y
if (y = yTarget and ilY != 0){
    if (place_free(xTarget,yTarget+ilY*gridSizeY/2)){
        yTarget += ilY*gridSizeY
        yTarget = round(yTarget/gridSizeY)*gridSizeY
    }
    ilY = 0
}


// update target
if (y != yTarget){
    var difY = yTarget - y;
    var mvSpdY = clamp(abs(difY)*0.5, 4, 4)*sign(difY);

    y += mvSpdY

    if (abs(difY) &lt;= 4) {
        y = yTarget
    }
}
